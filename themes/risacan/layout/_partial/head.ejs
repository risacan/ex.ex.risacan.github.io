<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<% if (is_post()) { %>
  <title><%= page.title %> | <%= config.title %></title>
<% } else { %>
  <title><%= config.title %></title>
<% } %>
<title><%= config.title %></title>
<%- js("js/script.js") %>
<%- css("css/style.css") %>
<%- favicon_tag("images/profile.png") %>
<%- partial("_partial/google_analytics") %>
<%- open_graph({
  twitter_id: config.risacan.twitter_id,
  twiter_site: config.risacan.twitter_site,
  image: config.risacan.image
}) %>
